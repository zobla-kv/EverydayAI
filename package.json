{
    "name": "everyday-ai",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": "18.12.1"
    },
    "scripts": {
        "start": "npm run build:dev",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "ng:dev": "cd ng && ng serve",
        "ng:dev:localhost": "cd ng && ng serve --host 0.0.0.0 --port 3500",
        "ng:prod": "cd ng && ng serve -c production",
        "build:dev": "cd ng && ng build --output-hashing none && node ../scripts/postBuild.js",
        "build:prod": "cd ng && ng build -c production --output-hashing none && node ../scripts/postBuild.js",
        "express:dev": "npx nodemon express/app.js",
        "express:prod": "node express/app.js",
        "express:dev:forever": "forever start express/config/forever.json"
    },
    "dependencies": {
        "@angular/animations": "^15.0.0",
        "@angular/cdk": "^15.0.3",
        "@angular/common": "^15.0.0",
        "@angular/compiler": "^15.0.0",
        "@angular/core": "^15.2.9",
        "@angular/fire": "^7.5.0",
        "@angular/forms": "^15.0.0",
        "@angular/material": "^15.0.3",
        "@angular/platform-browser": "^15.0.0",
        "@angular/platform-browser-dynamic": "^15.0.0",
        "@angular/router": "^15.0.0",
        "@elastic/elasticsearch": "^8.11.0",
        "@types/crypto-js": "^4.1.1",
        "@types/imagesloaded": "^4.1.6",
        "body-parser": "^1.20.2",
        "cloudinary": "^1.41.0",
        "crypto-js": "^4.1.1",
        "decimal.js": "^10.4.3",
        "express": "^4.17.1",
        "firebase-admin": "^11.5.0",
        "forever": "^4.0.3",
        "fs-extra": "^11.1.1",
        "imagesloaded": "^5.0.0",
        "multer": "^1.4.5-lts.1",
        "ngx-masonry": "^14.0.1",
        "ngx-page-scroll": "^10.0.0",
        "nodemailer": "^6.4.10",
        "pug": "^3.0.2",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.4",
        "@angular/cli": "15.2.9",
        "@angular/compiler-cli": "^15.0.0",
        "@types/jasmine": "~4.3.0",
        "dotenv": "^8.2.0",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "nodemon": "^2.0.4",
        "typescript": "~4.8.2"
    },
    "dependenciesComments": {
        "0": "Dependencies express, firebase-admin and nodemailer, multer, cloudinary, forever, body-parser, fs-extra, pug are for express app",
        "1": "devDependencies dotenv and nodemon are for express app"
    }
}
